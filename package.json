{
  "name": "@lumkani/view-model-api",
  "version": "2.5.0-beta.2",
  "repository": "git@github.com:Lumkani/vue-view-model-api.git",
  "author": "shailen <shailen@lumkani.com>",
  "license": "MIT",
  "main": "dist/view-model-api.ssr.js",
  "browser": "dist/view-model-api.esm.js",
  "module": "dist/view-model-api.esm.js",
  "unpkg": "dist/view-model-api.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "!src/serve-dev.*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "serve": "vue-cli-service serve src/serve-dev.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "test": "jest --verbose --coverage",
    "serve_docs": "pnpx vuepress dev docs",
    "build_docs": "cd docs/ && pnpx vuepress build && cd ..",
    "build_package": "pnpm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.7.7",
    "@babel/runtime": "^7.9.0",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.2.1",
    "@types/jest": "^25.1.5",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/test-utils": "^1.0.0-beta.32",
    "babel-core": "^7.0.0-bridge.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^25.2.6",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "codecov": "^3.6.5",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "husky": "^4.2.3",
    "jest": "^25.2.7",
    "minimist": "^1.2.0",
    "rollup": "^1.27.13",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-vue": "^5.1.5",
    "vue": "^2.6.11",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuelidate": "^0.7.5",
    "vuepress": "^1.4.0",
    "vuex": "^3.1.3"
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "engines": {
    "node": ">=8"
  },
  "husky": {
    "hooks": {
      "pre-push": "pnpx eslint --ext .js . && npx jest"
    }
  }
}
